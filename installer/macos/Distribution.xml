<?xml version="1.0" encoding="utf-8"?>
<installer-gui-script minSpecVersion="2">
    <title>eGuard Agent</title>
    <organization>com.eguard</organization>
    <domains enable_localSystem="true"/>
    <options customize="never" require-scripts="true" rootVolumeOnly="true"/>
    <volume-check>
        <allowed-os-versions>
            <os-version min="12.0"/>
        </allowed-os-versions>
    </volume-check>
    <choices-outline>
        <line choice="default">
            <line choice="com.eguard.agent"/>
        </line>
    </choices-outline>
    <choice id="default"/>
    <choice id="com.eguard.agent" visible="false">
        <pkg-ref id="com.eguard.agent"/>
    </choice>
    <pkg-ref id="com.eguard.agent" version="0.1.0" onConclusion="none">eguard-agent-component.pkg</pkg-ref>
</installer-gui-script>
